{
    "name": "joeyfrich/imageresizer",
    "description": "ImageResizer is an image resizing service built for the VictoryCTO LaravelSandbox coding exercise",
    "license": "license",
    "authors": [
        {
            "name": "Joey Rich",
            "email": "joeyfrich@gmail.com"
        }
    ],
    "homepage": "https://github.com/joeyfrich/imageresizer",
    "keywords": ["Laravel", "imageresizer"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "joeyfrich\\imageresizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "joeyfrich\\imageresizer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "joeyfrich\\imageresizer\\imageresizerServiceProvider"
            ],
            "aliases": {
                "imageresizer": "joeyfrich\\imageresizer\\Facades\\imageresizer"
            }
        }
    }
}
